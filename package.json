{
  "name": "@big-whale-labs/frontend-utils",
  "version": "0.0.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BigWhaleLabs/frontend-utils"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "np": {
    "2fa": false,
    "tests": false,
    "cleanup": false,
    "yarn": false
  },
  "scripts": {
    "start": "vite",
    "release": "np",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@big-whale-labs/constants": "^0.0.10",
    "@big-whale-labs/seal-cred-ledger-contract": "^0.0.4",
    "@preact/preset-vite": "^2.2.0",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "ethers": "^5.6.8",
    "np": "^7.6.1",
    "postcss": "^8.4.14",
    "preact": "^10.8.0",
    "prettier": "^2.6.2",
    "rollup": "^2.75.6",
    "rollup-plugin-dts": "^4.2.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.10",
    "vite-plugin-dts": "^1.2.0"
  },
  "packageManager": "yarn@3.2.0"
}
